            angular.element(document.getElementById('tripType'))[0].value = "2";
            $scope.airlines = "All";
            $scope.FlexibleSearch = "false";
            $scope.cabinClass = "Economy";
            $scope.adult = "1";
            $scope.child = "0";
            $scope.infant = "0";
            $scope.infantWs = "0";
            var mx = 2;
