            var xs = 7;
            returnsDate = new Date();
            returnsDate.setDate(returnsDate.getDate() + xs);
            var checkInDate_from = $.datepicker.formatDate('M dd, yy', checkInDate);
            var dt_from = $.datepicker.formatDate('M dd, yy', returnsDate);
