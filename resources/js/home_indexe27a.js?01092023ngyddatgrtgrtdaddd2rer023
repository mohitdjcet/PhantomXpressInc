         }
         $http.post('/getDeals', post_data).success(function (response) {
            if (!response.includes("DOCTYPE")) {
               if (response != 'none') {
                  $scope.froCity = response;
               }
            } else {
               $('#froCity').removeClass('input_cont x');
            }
         }).error(function (error) {})
